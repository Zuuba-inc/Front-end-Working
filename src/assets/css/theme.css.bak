/* The container */
.container {
  top: 26px;
  display: block;
  position: relative;
  padding-left:26px;
  padding-bottom: 13px;
  cursor: pointer;
  font-size: 12px;
  font-family: 'Open-Sans-Regular';
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.container-end{
