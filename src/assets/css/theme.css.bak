/* start onoffswitch*/
#main .list2 .onoffswitch {
  position: relative;
  height:20px;
  margin-top: 15px;
  margin-bottom: 10px;
  width: 95%;
  height: auto;
  -webkit-user-select:none;
   -moz-user-select:none;
    -ms-user-select: none;
}

#main .list2  .onoffswitch h3{
  position: absolute;
  color: #363636;
  font-size: 14px;
  line-height: 19px;

}
#main .list2 .onoffswitch-checkbox {
  display: none;
}
#main .list2 .onoffswitch-label {
  float:right;
  overflow: hidden;
  cursor: pointer; 
  border-radius: 20px;
  width: 42px;
  height: 20px;
  
}

#main .list2 .onoffswitch-inner {
  display: block;
  width: 200%; 
  margin-left: -100%;
  transition: margin 0.3s ease-in 0s;
}
#main .list2 .onoffswitch-inner:before, .onoffswitch-inner:after {
  display: block;
  float: left;
  width: 50%;
  height: 30px;
  padding: 0;
  line-height: 14px;
  font-size: 10px;
  color: white;
  font-family: 'Open-Sans-SemiBold';
  box-sizing: border-box;
}
#main .list2 .onoffswitch-inner:before {
  content: "ON";
  padding-left: 6px;
  padding-top:3px;
  background-color: #0B8B8C;
  color: #FFFFFF;
}
#main .list2 .onoffswitch-inner:after {
  content: "OFF";
  padding-right: 6px;
  padding-top:3px;
  background-color: #FFFFFF;
  color: #0B8B8C;
  text-align: right;
}
#main .list2 .onoffswitch-switch {
  display: block;
  width: 16px;
  height: 16px;
  background: #fff;
  position: absolute;
  border-radius: 20px;
  transition: all 0.3s ease-in 0s;
  margin-top: 2px;
  margin-right: 2px;
  margin-bottom: 2px;
  border: 1px solid #0B8B8C;
}
#main .list2 .onoffswitch-switch:after{
  background: #0B8B8C;
}
#main .list2 .onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-inner {
  margin-left: 0;
}
#main .list2 .onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-switch {
  margin-left: 26px;
}

/* end onoffswitch*/
